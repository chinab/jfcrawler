<?xml version="1.0" encoding="UTF-8"?>
<jfcrawler>
	<fetcher>
		<max-connection>2</max-connection>
		<max-pool-size>10</max-pool-size>
		<max-time-out>30000</max-time-out>
		<accessing-interval>1000</accessing-interval>
		<thread-interval>2000</thread-interval>;
	</fetcher>
	<crawler>
		<thread-interval>2000</thread-interval>
	</crawler>
	<writer>
		<root>jobs/</root>
	</writer>
	<url-database>
		<host></host>
		<user></user>
		<pass></pass>
	</url-database>
</jfcrawler>